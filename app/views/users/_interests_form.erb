		<!-- User Interests -->
		<div class="row">
			<div class="input-field col s12">
				<% @interests.all.each do |interest| %>
				    <% id = interest.id%>
						<div class="row">
					    <div class="col s3"
							<input id="<%=id%>" name="user[all_interests][]" value="<%=interest.name%>" type="checkbox">
					    <label for="<%=id%>"><%= interest.name%></label>
							</div>
						</div>
				  <% end %>
			</div>
		</div>
